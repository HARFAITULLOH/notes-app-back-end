{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style":"off",
        "no-console":"off",
        "no-unused-vars":"off",
        "no-var":"off",
        "indent":"off",
        "no-tabs":"off",
        "eol-last":"off",
        "keyword-spacing":"off",
        "consistent-return":"off",
        "no-else-return":"off",
        "no-underscore-dangle":"off",
        "class-methods-use-this":"off",
        "camelcase":"off"
    }
}
